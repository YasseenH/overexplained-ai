{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server/src/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "tsc": "npx tsc",
    "lint": "npx eslint .",
    "build": "npm-run-all clean tsc",
    "dev:start": "npm-run-all build start",
    "local": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "npm run prisma:init && node .",
    "prisma:init": "npx prisma generate && npx prisma db push",
    "test:unit": "DOTENV_CONFIG_PATH=./.env.test jest -i --verbose test/unit",
    "test:integration": "DOTENV_CONFIG_PATH=./.env.test jest -i --detectOpenHandles --forceExit test/integration",
    "prisma:ui": "npx prisma studio"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^5.1.0",
    "@prisma/client": "^6.11.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "4.18.2",
    "http-status": "1.5.3",
    "openai": "^5.10.2",
    "resend": "^4.6.0",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "4.17.14",
    "@types/http-errors": "2.0.1",
    "@types/jest": "29.2.3",
    "@types/node": "^18.11.9",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "eslint": "8.28.0",
    "jest": "29.3.1",
    "nodemon": "2.0.20",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.3",
    "prisma": "^6.11.1",
    "rimraf": "3.0.2",
    "supertest": "6.3.2",
    "ts-jest": "29.0.3",
    "tsx": "^4.20.3",
    "typescript": "^5.1.6",
    "undici-types": "^7.12.0"
  },
  "author": "",
  "license": "ISC"
}
